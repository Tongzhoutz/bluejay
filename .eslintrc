{
  "extends": "airbnb",
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "requireConfigFile": false,
    "babelOptions": {
      "presets": ["@babel/preset-react"]
    }
  },
  "rules": {
     "react/jsx-filename-extension": [1, {"extensions": [".js"] }]
  },
  "env": {
    "browser": true
  }
}